<%- include('../partials/header'); %>

<body class="container">
    <%- include('../partials/aside') %>
    <main class="content">
        
        <h1>Detail d'une parcelle</h1>
        
        <form action="<%= actionLink %>" method="post">
            <div class="field">
                <label class="label">Nom</label>
                <div class="control">
                    <input class="input" name="name" type="text" placeholder="Nom de la parcelle"  value="<%= plot.name %>" disabled>
                </div>
            </div>
            <%- include('../components/btn_form', {domain: 'plots', element: plot}) %>
        </form>
    </main>
</body>
</html>